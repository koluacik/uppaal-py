			<target ref="id1"/>
		</transition>
	</template>
	<system>// Place template instantiations here.
Process = Template();
// List one or more processes to be composed into a system.
system Process;
    </system>
	<queries>
		<query>
			<formula></formula>
			<comment>l0 3 l1 2 l2 1 l3 0 l0</comment>
		</query>
		<query>
