<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE nta PUBLIC '-//Uppaal Team//DTD Flat System 1.1//EN' 'http://www.it.uu.se/research/group/darts/uppaal/flat-1_2.dtd'>
<nta>
	<declaration>// Place global declarations here.</declaration>
	<template>
		<name x="5" y="5">Template</name>
		<declaration>// Place local declarations here.
clock x, y;</declaration>
		<location id="id0" x="442" y="0">
			<name x="432" y="-34">l2</name>
		</location>
		<location id="id1" x="238" y="0">
			<name x="228" y="-34">l1</name>
			<label kind="invariant" x="212" y="17">x - y &lt;= 3</label>
		</location>
		<location id="id2" x="0" y="0">
			<name x="-10" y="-34">l0</name>
			<label kind="invariant" x="-10" y="17">x &gt;= 4</label>
		</location>
		<init ref="id2"/>
		<transition>
			<source ref="id1"/>
			<target ref="id0"/>
			<label kind="guard" x="289" y="-25">x &gt;= 100</label>
		</transition>
		<transition>
			<source ref="id2"/>
			<target ref="id1"/>
			<label kind="assignment" x="102" y="-25">y = 0</label>
		</transition>
	</template>
	<system>// Place template instantiations here.
Process = Template();
// List one or more processes to be composed into a system.
system Process;
    </system>
	<queries>
		<query>
			<formula></formula>
			<comment>l0 1 l1 0 l2</comment>
		</query>
		<query>
			<formula></formula>
			<comment>0</comment>
		</query>
	</queries>
</nta>
