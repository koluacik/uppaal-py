<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE nta PUBLIC '-//Uppaal Team//DTD Flat System 1.1//EN' 'http://www.it.uu.se/research/group/darts/uppaal/flat-1_2.dtd'>
<nta>
	<declaration>// Place global declarations here.</declaration>
	<template>
		<name x="5" y="5">ta1</name>
		<declaration>// Place local declarations here.
clock x;</declaration>
		<location id="id0" x="0" y="0">
			<name x="-10" y="-34">l0</name>
			<label kind="invariant" x="-test" y="17">x &lt;= 5</label>
		</location>
		<location id="id1" x="204" y="0">
			<name x="194" y="-34">l2</name>
			<label kind="invariant" x="194" y="17">x &lt;= 100</label>
		</location>
		<location id="id2" x="204" y="136">
			<name x="194" y="102">l4</name>
			<label kind="invariant" x="194" y="153">x &gt;= 15</label>
		</location>
		<location id="id3" x="0" y="136">
			<name x="-10" y="102">l3</name>
			<label kind="invariant" x="-10" y="153">x &lt;= 15 &amp;&amp; x&gt;= 6</label>
		</location>
		<location id="id4" x="102" y="-102">
			<name x="92" y="-136">l1</name>
			<label kind="invariant" x="92" y="-85">x &gt;= 15 &amp;&amp; x &lt;= 20</label>
		</location>
		<location id="id5" x="102" y="238">
			<name x="92" y="204">l5</name>
			<label kind="invariant" x="92" y="255">x &lt;= 5</label>
		</location>
		<init ref="id0"/>
		<transition>
			<source ref="id0"/>
			<target ref="id3"/>
			<label kind="guard" x="0" y="34">x &lt;= 3</label>
			<label kind="assignment" x="0" y="68">x = 0</label>
		</transition>
		<transition>
			<source ref="id5"/>
			<target ref="id5"/>
			<label kind="guard" x="86" y="272">x == 5</label>
			<label kind="synchronisation" x="86" y="289">x = 0</label>
			<nail x="68" y="238"/>
			<nail x="68" y="306"/>
			<nail x="136" y="306"/>
			<nail x="136" y="238"/>
		</transition>
		<transition>
			<source ref="id3"/>
			<target ref="id2"/>
			<label kind="synchronisation" x="18" y="119">x &lt;= 10 &amp;&amp; x &gt;= 6</label>
		</transition>
		<transition>
			<source ref="id3"/>
			<target ref="id5"/>
		</transition>
		<transition>
			<source ref="id5"/>
			<target ref="id2"/>
		</transition>
		<transition>
			<source ref="id2"/>
			<target ref="id1"/>
		</transition>
		<transition>
			<source ref="id4"/>
			<target ref="id1"/>
			<nail x="204" y="-102"/>
		</transition>
		<transition>
			<source ref="id0"/>
			<target ref="id4"/>
			<label kind="guard" x="18" y="-85">x &gt;= 3</label>
			<label kind="assignment" x="18" y="-51">x = 0</label>
			<nail x="0" y="-102"/>
		</transition>
	</template>
	<system>// Place template instantiations here.
Process = Template();
// List one or more processes to be composed into a system.
system Process;
    </system>
	<queries>
		<query>
			<formula></formula>
			<comment></comment>
		</query>
	</queries>
</nta>
