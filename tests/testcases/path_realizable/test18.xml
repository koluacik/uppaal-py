<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE nta PUBLIC '-//Uppaal Team//DTD Flat System 1.1//EN' 'http://www.it.uu.se/research/group/darts/uppaal/flat-1_2.dtd'>
<nta>
	<declaration>// Place global declarations here.</declaration>
	<template>
		<name x="5" y="5">Template</name>
		<declaration>// Place local declarations here.
clock x;</declaration>
		<location id="id0" x="0" y="0">
			<name x="-10" y="-34">l0</name>
			<label kind="invariant" x="-10" y="17">x &lt;= 5</label>
		</location>
		<location id="id1" x="136" y="0">
			<name x="126" y="-34">l1</name>
			<label kind="invariant" x="126" y="17">x &gt;=5 &amp;&amp; x &lt;= 6</label>
		</location>
		<init ref="id0"/>
		<transition>
			<source ref="id0"/>
			<target ref="id1"/>
			<label kind="guard" x="8" y="-59">x &lt;= 7 &amp;&amp; x &gt;= 4</label>
		</transition>
	</template>
	<system>// Place template instantiations here.
Process = Template();
// List one or more processes to be composed into a system.
system Process;
    </system>
	<queries>
		<query>
			<formula></formula>
			<comment>l0 0 l1</comment>
		</query>
		<query>
			<formula></formula>
			<comment>0</comment>
		</query>
	</queries>
</nta>
